{
  "name": "anchart3d",
  "version": "1.0.0",
  "description": "A library featuring 3D-Charts which is built with the help of the three.js library",
  "main": "build/anchart3d.js",
  "directories": {
    "example": "examples",
    "test": "test"
  },
  "scripts": {
    "start": "webpack --progress --colors --watch",
    "minify": "webpack && uglifyjs ./build/anchart3d.js -c -m -o ./build/anchart3d.min.js --source-map ./build/anchart3d.min.js.map"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/deKilla/anchart3d.git"
  },
  "keywords": [
    "charts",
    "3d",
    "threejs",
    "anchart3d"
  ],
  "author": "Amar Bajric",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/deKilla/anchart3d/issues"
  },
  "homepage": "https://github.com/deKilla/anchart3d#readme",
  "devDependencies": {
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "expose-loader": "^0.7.1",
    "three": "^0.82.1",
    "tween.js": "^16.3.5",
    "uglify-js": "^2.7.5",
    "webpack": "^1.13.3"
  }
}
